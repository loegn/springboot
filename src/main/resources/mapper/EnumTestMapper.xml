<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.springboot.dao.EnumTestMapper">

    <resultMap id="BaseResultMap" type="com.example.springboot.entity.EnumTest">
        <result property="id" jdbcType="INTEGER" column="id"/>
        <result property="status" column="status"/>
        <result property="type" jdbcType="TINYINT" column="type"
                typeHandler="com.example.springboot.mybatis.handler.EnumTypeEnumsHandler"/>
        <result property="delete" column="delete"/>
    </resultMap>
<!--    <select id="selectById" resultType="com.example.springboot.entity.EnumTest">-->
    <select id="selectById" resultMap="BaseResultMap">
        select id,status,`delete`,`type` from enum_test where id = #{id}
    </select>
</mapper>
